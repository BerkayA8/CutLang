#!/bin/bash

set -e

notebook_env=$CONDA_DIR/envs/notebook
CUTLANG_PATH=$notebook_env/bin/cutlang

touch $CUTLANG_PATH/mybinder_true

jupyterConfigDir=$(jupyter --config-dir)
echo "::: $jupyterConfigDir is jupyterConfigDir"
echo "::: $(cat $jupyterConfigDir/jupyter_notebook_config.py)"

cp $CUTLANG_PATH/binder/jupyter_notebook_config.py $jupyterConfigDir/jupyter_notebook_config.py